   title = {Acomplamiento molecular:: criterios prácticos para la selección de ligandos biológicamente activos e identificación de nuevos blancos terapéuticos},
   volume = {7},
   url = {http://www.scielo.org.bo/scielo.php?script=sci_arttext&pid=S2310-02652019000200006&lng=es&nrm=iso&tlng=es http://www.scielo.org.bo/scielo.php?script=sci_abstract&pid=S2310-02652019000200006&lng=es&nrm=iso&tlng=es},
